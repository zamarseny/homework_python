AlexM [8:45 PM]
def parse_p(p):
   p = p.replace(',','.')
   return float(p) if float(p) < 1000 else float(p)/1000


summa = 0
with open('real_data.txt', 'r') as f:
   for line in f:
       try:
           d, user, p =  line.strip().split('\t')
       except:
           pass
       summa += parse_p(p)
print(f'{summa:.2f}')
точно вы правы
def parse_p(p):
   p = p.replace(',','.')
   return float(p) if float(p) < 1000 else float(p)/1000000
summa = 0
with open('real_data.txt', 'r') as f:
   for line in f:
       try:
           d, user, p =  line.strip().split('\t')
           summa += parse_p(p)
       except:
           pass

print(f'{summa:.2f}')

